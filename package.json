{
  "name": "sample openapi swagger project",
  "version": "1.0.0",
  "description": "Sample swaggerui + openapi + webpack + refs project",
  "scripts": {
    "build": "npm run combine && webpack",
    "start": "npm run combine && webpack-dev-server --open",
    "combine": "npx openapi-combine merge -i src/swagger-config.yaml -o ./output/swagger-config-merged.yaml",
    "export": "npx @openapitools/openapi-generator-cli generate -i src/swagger-config.yaml -g typescript-angular -o exported/"
  },
  "author": "mandarini",
  "license": "Apache-2.0",
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "html-webpack-plugin": "^5.5.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.1"
  },
  "dependencies": {
    "css-loader": "^6.5.1",
    "json-loader": "^0.5.7",
    "style-loader": "^3.3.1",
    "swagger-ui": "^4.1.3",
    "yaml-loader": "^0.6.0"
  }
}
